{
  "manifest_version": 3,
  "name": "U-FRET Ad Blocker",
  "version": "1.0.0",
  "description": "U-FRETサイトの広告要素を自動的に削除するChrome拡張機能",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}

